{
  "name": "ls-remote-versions",
  "version": "1.4.28",
  "description": "",
  "bin": {
    "ls-remote-versions": "bin/index.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib; npx babel src --out-dir lib --ignore \"src/**/test.js\" ",
    "watch": "babel --watch src --out-dir lib --ignore test.js",
    "lint": "standard --env jest src/**/*.js",
    "tests-only": "jest src/**/test.js",
    "coverage": "jest --coverage src/**/test.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghinks/ls-remote-versions.git"
  },
  "keywords": [
    "ls",
    "versions",
    "find",
    "module"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "isomorphic-fetch": "^3.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "npm-package-arg": "^8.0.0",
    "registry-url": "^5.0.0",
    "semver": "7.3.2"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/plugin-transform-runtime": "7.12.10",
    "@babel/preset-env": "7.12.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-rewire": "1.2.0",
    "jest": "26.6.3",
    "nock": "13.0.5",
    "nodemon": "2.0.6",
    "standard": "16.0.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "jest": true
    },
    "globals": [
      "__rewire_reset_all__"
    ]
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "testURL": "http://localhost"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "4.0.0"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-rewire",
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  }
}
