{
  "name": "ls-remote-versions",
  "version": "1.1.3",
  "description": "",
  "bin": "bin/index.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib; npx babel src --out-dir lib --ignore \"src/**/test.js\" ",
    "watch": "babel --watch src --out-dir lib --ignore test.js",
    "lint": "standard --env jest src/**/*.js",
    "test": "jest --coverage src/**/test.js"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghinks/ls-remote-versions.git"
  },
  "keywords": [
    "ls",
    "versions",
    "find",
    "module"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "npm-package-arg": "^6.0.0",
    "registry-url": "^3.1.0",
    "semver": "5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "babel-plugin-rewire": "^1.2.0",
    "jest": "^23.6.0",
    "nock": "^10.0.2",
    "nodemon": "^1.18.7",
    "standard": "^12.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "env": {
      "jest": true
    }
  },
  "jest": {
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "html"
    ],
    "testURL": "http://localhost"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "4.0.0"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ],
    "env": {
      "test": {
        "plugins": [
          "babel-plugin-rewire",
          "@babel/plugin-transform-runtime"
        ]
      }
    }
  }
}
